{
    "gitbook": "3.x.x",
    "title": "Swimlane",
    "root": "/docs",
    "plugins": [
        "highlight",
        "github",
        "edit-link",
        "gistrun",
        "theme-api",
        "include-codeblock",
        "versions"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/swimlane/angular2-data-table/"
        },
        "edit-link": {
            "base": "https://github.com/swimlane/angular2-data-table/tree/master",
            "label": "Edit"
        },
        "theme-api": {
            "theme": "dark"
        },
        "versions": {
            "type": "tags"
        }
    }
}
